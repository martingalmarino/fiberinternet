{
  "buildCommand": "echo 'Static site - no build step required'",
  "outputDirectory": ".",
  "redirects": [
    {
      "source": "/mobil.html",
      "destination": "/mobil",
      "permanent": true
    },
    {
      "source": "/tv.html",
      "destination": "/tv",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/mobil",
      "destination": "/mobil.html"
    },
    {
      "source": "/tv",
      "destination": "/tv.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/data/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ]
}
